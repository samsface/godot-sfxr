"language": "NativeScript",
"path": "res://addons/godot_sfxr/godot_sfxr_native.gdns"
}, {
"base": "EditorPlugin",
"class": "GodotSfxr",
"language": "GDScript",
"path": "res://addons/godot_sfxr/GodotSfxr.gd"
}, {
"base": "Reference",
